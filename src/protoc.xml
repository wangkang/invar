<?xml version="1.0" encoding="UTF-8"?>
<package name="invar.protoc">

	<Enum name="Gender">
		<MALE value="1" />
		<FEMALE value="2" />
		<NONE value="3" />
	</Enum>
	<Struct name="UserRole" doc="A game role created by a user.">
		<String name="name" value="INVAR" doc="Role name" />
		<Int8 name="age" value="30" />
		<Gender name="gender" />
		<Career name="career" />
		<Vec-invar.config::BagItem name="items" doc="All items in bag." />
		<Map-Gender-String name="genderStr" />
	</Struct>
	<Struct name="BagItem" doc="Items that can appear in user's bag.">
		<String name="name" />
		<String name="key" />
		<ItemType name="type" />
		<Int32 name="count" />
	</Struct>

	<Enum name="ItemType">
		<WEAPON value="1" />
		<RING value="2" />
		<NECKLACE value="18" />
	</Enum>

	<Enum name="LoginResult">
		<ERR value="1" />
		<OK value="2" />
	</Enum>
	<Protoc name="Login" doc="Login protocol.">
		<client>
			<String name="name" doc="User name. Reqired 6-14 chars." />
			<String name="passwd" doc="Password, 6 chars at least" />
			<Career name="careerEnum" doc="Five careers totally." />
		</client>
		<server>
			<LoginResult name="result" doc="" />
			<Vec-UserRole name="me" doc="" />
		</server>
	</Protoc>
</package>